.section {
  width: 690rpx;
  height: 200rpx;
  margin: 20rpx auto;
  border: 1px solid #eee;
  border-radius: 20rpx;
  box-shadow: 0 0 30rpx rgba(0, 0, 0, 0.06);
  display: flex;
  overflow: hidden;
}
.section .left {
  flex-basis: 28%;
  background-color: #7ca0ec;
}
.section .left .text1 {
  color: #eee;
  font-weight: bold;
  font-size: 40rpx;
  margin-left: 10rpx;
  margin-top: 10rpx;
}
.section .left .text2 {
  color: #eee;
  font-size: 29rpx;
  margin-left: 10rpx;
  margin-top: 10rpx;
}
.section .left .goto_find {
  background-color: #eee;
  width: 70%;
  border-radius: 20rpx;
  text-align: center;
  color: #7ca0ec;
  margin-top: 20rpx;
  margin-left: 10rpx;
}
.section .right {
  flex-basis: 72%;
}
.section .right .my_scroll {
  white-space: nowrap;
  width: 496rpx;
  height: 200rpx;
}
.section .right .my_scroll ::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
}
.section .right .my_scroll .block_item {
  height: 200rpx;
  width: 200rpx;
  font-size: 38rpx;
  overflow: hidden;
  display: inline-block;
}
.section .right .my_scroll .block_item .block_item_up {
  margin-top: 20rpx;
  margin-left: 20rpx;
  height: 65rpx;
  width: 84%;
  background-color: #fefbf6;
  border-radius: 15rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.section .right .my_scroll .block_item .block_item_down {
  margin-top: 30rpx;
  margin-left: 20rpx;
  height: 65rpx;
  width: 84%;
  background-color: #fefbf6;
  border-radius: 15rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.section .section_body {
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  padding: 5rpx 0;
}
.section .section_body .section_body_item {
  width: 25%;
  height: 200rpx;
  text-align: center;
  box-sizing: border-box;
}
.section .section_body .section_body_item .section_body_item_img {
  width: 96rpx;
  height: 96rpx;
  margin-top: 30rpx;
  margin-left: 40rpx;
}
.section .section_body .section_body_item .section_body_item_img .images {
  width: 96rpx;
  height: 96rpx;
}
.section .section_body .section_body_item .section_body_item_title {
  width: 100%;
  height: 34rpx;
  line-height: 34rpx;
  color: #06121e;
  font-size: 30rpx;
  font-weight: bold;
  margin-top: 20rpx;
}
.quick_section {
  width: 690rpx;
  height: 310rpx;
  margin: 20rpx auto;
  border: 1px solid #eee;
  border-radius: 20rpx;
  box-shadow: 0 0 30rpx rgba(0, 0, 0, 0.06);
  display: flex;
  overflow: hidden;
  flex-wrap: wrap;
  flex-direction: row;
}
.quick_section .quick_section_item {
  width: 50%;
  height: 150rpx;
  border-radius: 20rpx;
  overflow: hidden;
  text-align: center;
}
.quick_section .quick_section_item .quick_box {
  display: flex;
  width: 90%;
  height: 150rpx;
  border-radius: 20rpx;
  margin-left: 16rpx;
}
.quick_section .quick_section_item .quick_box .quick_section_item_left {
  padding-left: 15rpx;
  padding-top: 25rpx;
}
.quick_section .quick_section_item .quick_box .quick_section_item_left .quick_big {
  font-size: 44rpx;
  font-weight: bold;
}
.quick_section .quick_section_item .quick_box .quick_section_item_left .quick_small {
  font-size: 30rpx;
}
.quick_section .quick_section_item .quick_box .quick_section_item_right {
  padding-top: 20rpx;
  padding-left: 5rpx;
  width: 100rpx;
  height: 100rpx;
}
.quick_section .quick_section_item .quick_box .quick_section_item_right .quick_images {
  width: 100rpx;
  height: 100rpx;
}
.doc_container {
  width: 690rpx;
  margin: 20rpx auto;
  border: 1px solid #eee;
  border-radius: 20rpx;
  box-shadow: 0 0 30rpx rgba(0, 0, 0, 0.06);
}
.doc_container .doc_title {
  font-size: 50rpx;
  font-weight: bold;
  padding-top: 5rpx;
  padding-left: 10rpx;
  margin-bottom: 15rpx;
}
.doc_container .doc_swiper {
  height: 520rpx;
}
.doc_container .doc_swiper-item .doc_content {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content: space-between;
}
.doc_container .doc_swiper-item .doc_content .doc_card {
  margin: 5rpx 10rpx;
  height: 250rpx;
  width: 660rpx;
  border-radius: 20rpx;
  display: flex;
  box-shadow: 0 0 30rpx rgba(0, 0, 0, 0.1);
}
.doc_container .doc_swiper-item .doc_content .doc_card .doc_left_img {
  margin: 20rpx 20rpx;
  width: 200rpx;
  height: 200rpx;
}
.doc_container .doc_swiper-item .doc_content .doc_card .doc_left_img .doc_images {
  width: 180rpx;
  height: 180rpx;
  border-radius: 50%;
}
.doc_container .doc_swiper-item .doc_content .doc_card .doc_right_info .doc_info_top {
  width: 420rpx;
  height: 66rpx;
  overflow: hidden;
  padding-top: 20rpx;
}
.doc_container .doc_swiper-item .doc_content .doc_card .doc_right_info .doc_info_top .doc_name {
  font-weight: bold;
  font-size: 50rpx;
  margin-right: 10rpx;
}
.doc_container .doc_swiper-item .doc_content .doc_card .doc_right_info .doc_info_top .doc_description {
  font-size: 28rpx;
}
.doc_container .doc_swiper-item .doc_content .doc_card .doc_right_info .doc_info_bom {
  margin-top: 5rpx;
}
.doc_container .doc_swiper-item .doc_content .doc_card .doc_right_info .doc_info_bom .doc_info_bom_up {
  font-size: 28rpx;
  font-style: italic;
  margin-bottom: 5rpx;
}
.doc_container .doc_swiper-item .doc_content .doc_card .doc_right_info .doc_info_bom .doc_info_bom_down {
  width: 420rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
